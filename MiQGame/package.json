{
	"name": "miquickgame-2",
	"version": "1.0.0",
	"scripts": {
		"build": "SET NODE_OPTIONS=--openssl-legacy-provider && quickgame build --include-file-ext .data,.wasm,.br",
		"release": "SET NODE_OPTIONS=--openssl-legacy-provider && quickgame release --include-file-ext .data,.wasm,.br",
		"server": "SET NODE_OPTIONS=--openssl-legacy-provider && quickgame server",
		"debug": "SET NODE_OPTIONS=--openssl-legacy-provider && quickgame debug"
	},
	"dependencies": {
		"@babel/runtime-corejs2": "^7.5.5",
		"quickgame-cli": "^0.2.5"
	},
	"browser": {
		"crypto": false,
		"path": false,
		"fs": false
	},
	"babel": {
		"plugins": [
			"babel-plugin-transform-remove-strict-mode"
		],
		"@babel/core": "^7.4.5",
		"@babel/plugin-transform-runtime": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"babel-loader": "^8.0.6",
		"babel-eslint": "^8.2.1"
	}
}
